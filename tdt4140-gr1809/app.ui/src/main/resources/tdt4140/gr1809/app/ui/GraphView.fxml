<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="750" prefWidth="1000" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tdt4140.gr1809.app.ui.GraphViewController">
   <children>
      <Button fx:id="backButton" layoutX="1.0" layoutY="3.0" mnemonicParsing="false" onAction="#goBackToFxApp" text="Back" />
      <LineChart fx:id="heartRateChart" layoutX="1.0" layoutY="29.0" prefHeight="722.0" prefWidth="1000.0" title="Heart Rate">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" upperBound="200.0" />
        </yAxis>
      </LineChart>
   </children>
</Pane>
